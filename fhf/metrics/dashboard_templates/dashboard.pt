<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="leam.isop">


<metal:slot fill-slot="style_slot">
  <link rel="stylesheet" class="required"
    tal:define="navroot context/@@plone_portal_state/navigation_root_url"
    tal:attributes="href 
        string:${navroot}/++resource++fhf.metrics/dashboard.css">
</metal:slot>

<metal:slot fill-slot="javascript_head_slot">
  <script type="text/javascript"  
    tal:define="navroot context/@@plone_portal_state/navigation_root_url"
    tal:attributes="href 
        string:${navroot}/++resource++fhf.metrics/tabletop.js">
</metal:slot>

<body>

  <!-- remove title and description -->
  <metal:slot fill-slot="main">
    <metal:main define-macro="main">
      <div class="row">

        <!-- navigation sidebar -->
        <div class="col_sm_3 col_md_2 sidebar">
          DASHBOARD
        </div>

        <!-- metric display -->
        <div class="col_sm_9 col_md_10">
        </div>
      </div>
    </metal:main>
  </metal:slot>



